{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node",
			"linux": {
				"program": "/usr/bin/hoobsd",
				"args": [
					"*",
					"-d"
				],
				"env": {
					"FORCE_COLOR": "1",
					"DEBUG": "homebridge-automower-platform",
					"NODE_ENV": "development"
				}
			}
		}
	]
}